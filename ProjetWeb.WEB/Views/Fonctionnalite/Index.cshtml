@model List<ProjetWeb.MODEL.FonctionnaliteModel>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Index";

}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            <h4>ID Fonctionnalite</h4>
        </th>
        <th>
            <h4>Nom Fonctionnalite</h4>
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID_Fonctionnalite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nom_Fonctionnalite)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.ID_Fonctionnalite }) |
            </td>
        </tr>
    }

</table>
